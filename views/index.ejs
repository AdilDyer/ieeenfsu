  const text =
    "IEEE NFSU stands at the forefront of pioneering excellence, pushing the boundaries of forensic science research, education, and practice. It represents a beacon of knowledge and innovation in the field of forensic science.";
  const delay = 50; // Adjust the typing speed (milliseconds per character)
  function typeText() {
    if (index < text.length) {
      document.getElementById("typed-text").innerHTML += text.charAt(index);
      index++;
      setTimeout(typeText, delay);
    }
  }

  let index = 0;

  document.addEventListener("DOMContentLoaded", () => {
    let no = 23 * 1;
    if (window.scrollY == 0) {
      no += window.scrollY;
    } //if i make the no being calculated at the time of dom load then it actually works everytime otherwise it sometimes works and sometimes doesnt
    let firstVideo = document.querySelector("#firstvideo");
    firstVideo.style.marginLeft = no + "rem";

    typeText();
  });
</script>
